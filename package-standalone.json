{
  "name": "prepost-standalone",
  "version": "2.0.0",
  "description": "AI-Powered Social Media Protection - Standalone Version for Manus",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "mkdir -p data && chmod 755 data"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "next": "14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.22.4",
    "lru-cache": "^10.2.0",
    "framer-motion": "^11.0.8",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "lucide-react": "^0.344.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.4.5",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.33",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "social-media",
    "content-analysis",
    "risk-detection",
    "standalone",
    "manus"
  ],
  "author": "PREPOST Team",
  "license": "MIT"
}
